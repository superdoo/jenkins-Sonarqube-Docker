@Library('my-global-shared-library')
pipeline {
    agent any
    stages {
        stage('something to do') {
            steps {
                // Call a function from the shared library
                mySharedFunction()
            }
        }
        
        stage('greetings') {
            steps {
                script {
                    // Call the function from the shared library
                    my-global-shared-library.greet('Alice')
                }
            }
        }
    }
}
        
